{% extends 'base.html.twig' %} {# same layout as base #}



{% block body %} 
<div class="container mt-5">
    <div class="d-flex justify-content-evenly mx-1">
{% for agency in agencies %}
        <div class="card" style="width: 18rem;">
            <img src="..." class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">{{address}}</h5>
                <p class="card-text">City 🏙: {{agency.city}}</p>
                <p class="card-text">Street 🏢: {{agency.address}}</p>
                <p class="card-text">Phone Number 📞: {{agency.phone}}</p>
                <p class="card-text">Employes 👥: {{agency.employes}}</p>
            </div>
        </div>
{% endfor %}
    </div>
</div>

